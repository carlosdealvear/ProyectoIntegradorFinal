<% if(usuarioLogueado != null){ %>

	<%-include("./partials/headerLogueado") %>

	<% } else { %>

	<%-include("./partials/header")%>
	
<%}%>

<body>
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de Busqueda:</h2>
			</div>
	<% for(let i=0; i < productos.length; i++) { %> 
		<div class="col-12 col-sm-6 col-lg-3">
		<section class="product-box">
			<a href="http://localhost:3000/producto/id/<%= productos[i].id %>">
				<figure class="product-box_image">
					<img src="<%= productos[i].url %>" alt="<%= productos[i].nombre %>">
				</figure>
				<article class="product-box_data">
					<h2><%= productos[i].nombre %></h2>
					<h3 class="precios">$ <%= productos[i].precio %></h3>
					<p><%= productos[i].descripcion %></p>
					<p>Comentarios: 12</p>
				</article>
			</a>
		</section>
	</div>
	<% } %> 
</div>
	<%- include('./partials/footer') %> 
</body>

</html>